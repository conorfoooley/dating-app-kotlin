<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.main.view.MainActivity">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/content_main_rv_feed"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="@dimen/card_margin_fix_rview_8"
        android:layout_marginTop="@dimen/general_margin_16"
        android:layout_marginEnd="@dimen/general_margin_16"
        android:layout_marginBottom="@dimen/card_margin_fix_rview_8"
        android:background="@color/baseline"
        android:overScrollMode="never"
        app:layoutManager="GridLayoutManager"
        app:spanCount="2"
        tools:listitem="@layout/fragment_feed_rv_item" />

</LinearLayout>
