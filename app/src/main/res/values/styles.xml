<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">

        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="actionModeBackground">@color/actionModeBackground</item>

    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />



    <!-- basic tv -->
    <style name="regular_text_white_16sp_85op" parent="Widget.AppCompat.TextView">
        <item name="android:textColor">@color/white_85</item>
        <item name="android:fontFamily">@font/nunito_sans_regular</item>
        <item name="android:textSize">16sp</item>
    </style>

    <!-- header 3 -->
    <style name="regular_text_white_20sp_85op" parent="Widget.AppCompat.TextView">
        <item name="android:textColor">@color/white_85</item>
        <item name="android:fontFamily">@font/nunito_sans_regular</item>
        <item name="android:textSize">20sp</item>
    </style>


</resources>